{
  "name": "@silverback/theme_svbk",
  "author": "Silverback Studio",
  "license": "ISC",
  "repository": "github:silverbackstudio/_svbk",
  "version": "1.0.0",
  "description": "Silverback Starter Theme",
  "main": "index.php",
  "scripts": {
    "sass": "sass --watch ./sass:./",
    "blocks": "wp-scripts start blocks/blocks.js --output-path=dist/js",
    "blocks:build": "wp-scripts build blocks/blocks.js --output-path=dist/js",  
    "i18n-make-pot": "wp i18n make-pot ./  languages/_svbk.pot --domain=_svbk --exclude='node_modules,blocks/' && echo 'Did you remember to compile blocks for dev before pot-ing?'",
    "i18n-make-mo": "for f in languages/*.po; do FILENAME=${f%%.*}; msgfmt -o ${FILENAME}.mo ${FILENAME}.po; done;",
    "i18n-make-json": "wp i18n make-json languages/*.po --no-purge && cd languages && ls *.json | grep -v '_svbk' |  xargs -I {} mv {} _svbk-{} && cd ..",
    "translations": "npm run i18n-make-pot && npm run make-i18n-json",
    "build": "gulp build && npm run blocks:build"
  },
  "devDependencies": {
    "@wordpress/scripts": "^5.0.1",
    "autoprefixer": "^6.7.0",
    "classnames": "^2.2.6",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^2.0.0",
    "gulp-minify": "^3.1.0",
    "gulp-postcss": "^6.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-responsive": "^2.14.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-svgmin": "^2.2.0",
    "lodash": "^4.17.10",
    "node-sass": "^4.13.0",
    "node-sass-json-importer": "^4.1.0",
    "postcss-object-fit-images": "^1.1.2",
    "readline-sync": "^1.4.10"
  },
  "dependencies": {
    "@react-google-maps/api": "^1.7.11",
    "fg-loadcss": "^2.1.0",
    "sharp": "^0.23.4"
  }
}
